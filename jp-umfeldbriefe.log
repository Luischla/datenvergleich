+<?xml version="1.0" encoding="UTF-8"?>
+<TEI xmlns="http://www.tei-c.org/ns/1.0" xmlns:telota="http://www.telota.de" telota:doctype="letter" xml:id="ed_fz5_lzk_pkb">
+    <teiHeader>
+        <fileDesc>
+            <titleStmt>
+                <title>Von Gerson Adersbach an Max Richter. Bayreuth, 28. September 1820, Donnerstag</title>
+                <respStmt>
+                    <persName>
+                        <surname>Rölcke</surname>
+                        <forename>Michael</forename>
+                    </persName>
+                    <resp><note type="remarkResponsibility">Bearbeiter/in</note></resp>
+                </respStmt>
+            </titleStmt>
