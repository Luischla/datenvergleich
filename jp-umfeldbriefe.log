+<?xml version="1.0" encoding="UTF-8"?>
+<TEI xmlns="http://www.tei-c.org/ns/1.0" xmlns:telota="http://www.telota.de" telota:doctype="letter" xml:id="ed_ppz_bwg_nkb">
+    <teiHeader>
+        <fileDesc>
+            <titleStmt>
+                <title>Von Karl Spazier an Karl August Böttiger. Dessau, 10. Juli 1800,
+                    Donnerstag</title>
+                <respStmt>
+                    <persName>
+                        <surname>Kumpfmüller</surname>
+                        <forename>Leon</forename>
+                    </persName>
