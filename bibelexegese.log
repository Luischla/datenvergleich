+<?xml version="1.0" encoding="UTF-8"?>
+<?xml-model href="file:/C:/Users/stockhausen/Documents/Severian/schema/tei-pta.rng" schematypens="http://relaxng.org/ns/structure/1.0"?><TEI xmlns="http://www.tei-c.org/ns/1.0">
