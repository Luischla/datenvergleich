+<?xml version="1.0" encoding="UTF-8"?>
+<TEI xmlns="http://www.tei-c.org/ns/1.0" xmlns:telota="http://www.telota.de" telota:doctype="letter" xml:id="ed_x41_gzp_vjb">
+    <teiHeader>
+        <fileDesc>
+            <titleStmt>
+                <title>Von Caroline Richter an Johann Heinrich Voß. Bayreuth, 11. bis 19. September
+                    <?oxy_comment_start author="roelcke" timestamp="20191219T154211+0100" comment="doch wohl 1819, da war Jean Paul in Löbichau"?>1818<?oxy_comment_end?></title>
+                <title>, </title>
+                <title>Freitag bis Sonnabend</title>
+                <respStmt>
+                    <persName>
+                        <surname>Kumpfmüller</surname>
+                        <forename>Leon</forename>
+                    </persName>
+                    <resp><note type="remarkResponsibility">Bearbeiter/in</note></resp>
+                </respStmt>
+            </titleStmt>
+            <publicationStmt>
+                <publisher>Herausgebende Institution</publisher>
+            </publicationStmt>
+            <sourceDesc>
+                <listWit>
+                    <witness>
+                        <msDesc rend="manuscript" type="baseText">
+                            <msIdentifier>
+                                <institution><?oxy_custom_start type="oxy_content_highlight" color="153,255,255"?>BNUS<?oxy_custom_end?></institution>
