diff --git a/home/gordon/merge122_102/db/projects/lehndorff/data/Dokumente/18. Jahrhundert/1776-11_Tagebuch.xml.bak b/home/gordon/merge122_102/db/projects/lehndorff/data/Dokumente/18. Jahrhundert/1776-11_Tagebuch.xml.bak
